      }
    },
    "dependencies": ["L1/ExecutionClient"]
  },
  "category": "L2/ArbitrumOne",
  "documentation": {
    "default": "https://arbitrum.io/",
    "docker": "https://developer.offchainlabs.com/docs/running_node"
